{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [

		{
			"name": "Netlify Debugging",
			"type": "node",
			"request": "launch",
			"program": "netlify dev",
			"runtimeArgs": [
				"run-script",
				"debug-netlify"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			]
		},
		{
			"name": "Vercel Debugging",
			"type": "node",
			"request": "launch",
			"program": "vercel dev",
			"runtimeArgs": [
				"run-script",
				"debug-vercel"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			]
		},
		{
			"name": "Netlify Debugging - Live",
			"type": "node",
			"request": "launch",
			"program": "netlify dev --live",
			"runtimeArgs": [
				"run-script",
				"debug"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			]
		},
		{
			"name": "Vitest debug",
			"request": "launch",
			"runtimeArgs": [
				"run-script",
				"test"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"type": "node"
		},
		{
			"name": "script.ts",
			"request": "launch",
			"runtimeArgs": [
				"run-script",
				"test"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"type": "node"
		},
	]
}